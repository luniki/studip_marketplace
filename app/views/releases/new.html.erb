<h1>New release</h1>

<% semantic_form_for([@plugin, @release], :html => {:multipart => true}) do |form| %>
  <%= form.semantic_errors %>

  <% form.inputs do %>
    <%= form.input :package %>
  <% end %>
  <% form.buttons do %>
    <%= form.commit_button "Create" %>
  <% end %>
<% end %>

<%= link_to 'Back', plugin_releases_path(@plugin) %>
